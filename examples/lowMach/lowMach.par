[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7 
stopAt = numSteps
numSteps = 300
dt = 1e-03
timeStepper = bdf2
writeInterval = 100

[PROBLEMTYPE]
equation = lowMachNS
variableProperties = yes

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-10

[VELOCITY]
boundaryTypeMap = fixedValue
residualTol = 1e-12

[TEMPERATURE] 
boundaryTypeMap = fixedValue
residualTol = 1e-12
