[OCCA]
backend = SERIAL
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7 
stopAt = numSteps
numSteps = 300
dt = 1e-03
timeStepper = tombo2
writeInterval = 100

[PROBLEMTYPE]
equation = lowMachNS
variableProperties = yes

[PRESSURE]
residualTol = 1e-10
residualProj = false

[VELOCITY]
boundaryTypeMap = fixedValue
residualTol = 1e-12

[TEMPERATURE] 
boundaryTypeMap = fixedValue
residualTol = 1e-12
