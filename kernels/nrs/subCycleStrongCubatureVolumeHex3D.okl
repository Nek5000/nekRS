#if p_knl == 11
#if p_cubNq <= 13
// reduce reg
@kernel void subCycleStrongCubatureVolumeHex3D_v11(const dlong Nelements,
                                                   @restrict const dlong *elementList,
                                                   @restrict const dfloat *cubD,
                                                   @restrict const dfloat *cubInterpT,
                                                   const dlong offset,
                                                   const dlong cubatureOffset,
                                                   const dlong NUoffset,
                                                   @restrict const dfloat *invLumpedMassMatrix,
                                                   @restrict const dfloat *BdivW,
                                                   const dfloat c0,
                                                   const dfloat c1,
                                                   const dfloat c2,
                                                   @restrict const dfloat *conv,
                                                   @restrict const dfloat *Ud,
                                                   @restrict dfloat *NU)
{

#if p_cubNq == 16 || p_cubNq == 8
#define p_cubPad 1
#else
#define p_cubPad 0
#endif

#if p_Nq == 16 || p_Nq == 8 || p_Nq == 12
#define p_Pad 1
#else
#define p_Pad 0
#endif

#if p_cubNq == 3
#define p_Nblk 28
#elif p_cubNq == 4
#define p_Nblk 8
#elif p_cubNq == 5
  //#define p_Nblk 5
#define p_Nblk 10
#elif p_cubNq == 6
  //#define p_Nblk 3
#define p_Nblk 7
#elif p_cubNq == 7
  //#define p_Nblk 1
#define p_Nblk 5
#elif p_cubNq == 8
#define p_Nblk 2
#elif p_cubNq == 9
#define p_Nblk 3
#elif p_cubNq == 10
#define p_Nblk 1
#elif p_cubNq == 11
#define p_Nblk 1
#else
#define p_Nblk 1
#endif

  // (phi, U.grad Ud)
  for (dlong eo = 0; eo < Nelements; eo += p_Nblk; @outer(0)) {
    @shared dfloat s_cubD[p_cubNq][p_cubNq + p_cubPad];
    @shared dfloat s_cubInterp[p_cubNq][p_Nq + p_Pad];
    @exclusive dfloat r_Ud[p_Nq], r_Vd[p_Nq], r_Wd[p_Nq];

    @shared dfloat s_tmpU[p_Nblk][p_Nq][p_cubNq][p_cubNq + p_cubPad];
    @shared dfloat s_tmpV[p_Nblk][p_Nq][p_cubNq][p_cubNq + p_cubPad];
    @shared dfloat s_tmpW[p_Nblk][p_Nq][p_cubNq][p_cubNq + p_cubPad];

    @shared dfloat s_c[p_nEXT];
    @exclusive dlong element;

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
          const int id = i + j * p_cubNq;
          dlong e = es + eo;
          element = (e < Nelements) ? elementList[e] : -1;

          if (es == 0) {
            if (i == 0 && j == 0) {
              //#pragma unroll p_nEXT
              for (int s = 0; s < p_nEXT; s++) {
                dfloat coeff = 0;
                if (s == 0)
                  coeff = c0;
                if (s == 1)
                  coeff = c1;
                if (s == 2)
                  coeff = c2;
                // r_c[s] = coeff;
                s_c[s] = coeff;
              }
            }

            if (id < p_Nq * p_cubNq) {
              //	  s_cubInterpT[j][i] = cubInterpT[id];
              s_cubInterp[i][j] = cubInterpT[id];
            }
            s_cubD[j][i] = cubD[id];
          }
        }
      }
    } // es

    @barrier();

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
        for (int a = 0; a < p_cubNq; ++a; @inner(0)) {
          if (a < p_Nq && b < p_Nq && element != -1) {

            for (int c = 0; c < p_Nq; ++c) {
              const dlong id = element * p_Np + c * p_Nq * p_Nq + b * p_Nq + a;
              s_tmpU[es][c][b][a] = Ud[id + 0 * offset];
              s_tmpV[es][c][b][a] = Ud[id + 1 * offset];
              s_tmpW[es][c][b][a] = Ud[id + 2 * offset];
            }
          }
        }
      }
    } // es

    @barrier();

    // interpolate in 'a'
    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

          if (b < p_Nq && element != -1) {

#pragma unroll p_cubNq
            for (int c = 0; c < p_Nq; ++c) {
              r_Ud[c] = 0;
              r_Vd[c] = 0;
              r_Wd[c] = 0;
            }

#pragma unroll p_Nq
            for (int a = 0; a < p_Nq; ++a) {
              const dfloat Iia = s_cubInterp[i][a];
#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {
                r_Ud[c] += Iia * s_tmpU[es][c][b][a];
                r_Vd[c] += Iia * s_tmpV[es][c][b][a];
                r_Wd[c] += Iia * s_tmpW[es][c][b][a];
              }
            }
          }
        }
      }
    } // es

    @barrier();

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

          if (b < p_Nq && element != -1) {

            for (int c = 0; c < p_Nq; ++c) {
              s_tmpU[es][c][b][i] = r_Ud[c];
              s_tmpV[es][c][b][i] = r_Vd[c];
              s_tmpW[es][c][b][i] = r_Wd[c];
              r_Ud[c] = 0;
              r_Vd[c] = 0;
              r_Wd[c] = 0;
            }
          }
        }
      }
    } // es

    @barrier();

    // interpolate in 's'
    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

#pragma unroll p_cubNq
          for (int c = 0; c < p_Nq; ++c) {
            r_Ud[c] = 0;
            r_Vd[c] = 0;
            r_Wd[c] = 0;
          }

          // interpolate in b
#pragma unroll p_Nq
          for (int b = 0; b < p_Nq; ++b) {
            const dfloat Ijb = s_cubInterp[j][b];
#pragma unroll p_Nq
            for (int c = 0; c < p_Nq; ++c) {
              r_Ud[c] += Ijb * s_tmpU[es][c][b][i];
              r_Vd[c] += Ijb * s_tmpV[es][c][b][i];
              r_Wd[c] += Ijb * s_tmpW[es][c][b][i];
            }
          }
        }
      }
    } // es

    @barrier();

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
          for (int c = 0; c < p_Nq; ++c) {
            s_tmpU[es][c][j][i] = r_Ud[c];
            s_tmpV[es][c][j][i] = r_Vd[c];
            s_tmpW[es][c][j][i] = r_Wd[c];
            r_Ud[c] = 0;
            r_Vd[c] = 0;
            r_Wd[c] = 0;
          }
        }
      }
    } // es

    @shared dfloat s_sliceU[p_Nblk][p_cubNq][p_cubNq + p_cubPad];
    @shared dfloat s_sliceV[p_Nblk][p_cubNq][p_cubNq + p_cubPad];
    @shared dfloat s_sliceW[p_Nblk][p_cubNq][p_cubNq + p_cubPad];

#ifndef gfxXX
#pragma unroll p_Nq
#else
#pragma nounroll
#endif
    for (int k = 0; k < p_cubNq; ++k) {
      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

            dfloat Udk = 0, Vdk = 0, Wdk = 0;
#pragma unroll p_cubNq
            for (int c = 0; c < p_Nq; ++c) {
              const dfloat Ikc = c_I[k][c];

              Udk += Ikc * s_tmpU[es][c][j][i];
              Vdk += Ikc * s_tmpV[es][c][j][i];
              Wdk += Ikc * s_tmpW[es][c][j][i];
            }
            s_sliceU[es][j][i] = Udk;
            s_sliceV[es][j][i] = Vdk;
            s_sliceW[es][j][i] = Wdk;
          }
        }
      } // es

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            if (element != -1) {

              dfloat Udr = 0, Uds = 0, Udt = 0;
              dfloat Vdr = 0, Vds = 0, Vdt = 0;
              dfloat Wdr = 0, Wds = 0, Wdt = 0;

#pragma unroll p_Nq
              for (int n = 0; n < p_Nq; ++n) {
                dfloat Dkn = c_DI[k][n];

                Udt += Dkn * s_tmpU[es][n][j][i];
                Vdt += Dkn * s_tmpV[es][n][j][i];
                Wdt += Dkn * s_tmpW[es][n][j][i];
              }

#pragma unroll p_cubNq
              for (int n = 0; n < p_cubNq; ++n) {
                dfloat Din = s_cubD[i][n];
                dfloat Djn = s_cubD[j][n];

                Udr += Din * s_sliceU[es][j][n];
                Vdr += Din * s_sliceV[es][j][n];
                Wdr += Din * s_sliceW[es][j][n];

                Uds += Djn * s_sliceU[es][n][i];
                Vds += Djn * s_sliceV[es][n][i];
                Wds += Djn * s_sliceW[es][n][i];
              }

              dfloat Uhat = 0.0;
              dfloat Vhat = 0.0;
              dfloat What = 0.0;

              const int id = element * p_cubNp + k * p_cubNq * p_cubNq + j * p_cubNq + i;

              //#pragma unroll p_nEXT
              for (int s = 0; s < p_nEXT; ++s) {
                const int s_offset = s * p_NVfields * cubatureOffset;
                Uhat += s_c[s] * conv[id + 0 * cubatureOffset + s_offset];
                Vhat += s_c[s] * conv[id + 1 * cubatureOffset + s_offset];
                What += s_c[s] * conv[id + 2 * cubatureOffset + s_offset];
              }

              // I_f^t*(J_f*C_f^t)*G_f*\hat{D}_f*I_f*u
              dfloat foo1 = Uhat * Udr + Vhat * Uds + What * Udt;
              dfloat foo2 = Uhat * Vdr + Vhat * Vds + What * Vdt;
              dfloat foo3 = Uhat * Wdr + Vhat * Wds + What * Wdt;

#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {
                dfloat Ikc = c_I[k][c];
                r_Ud[c] += Ikc * foo1;
                r_Vd[c] += Ikc * foo2;
                r_Wd[c] += Ikc * foo3;
              }
            }
          }
        }
      } // es
    }   // k

    // now project back in 'b'
    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

#pragma unroll p_Nq
          for (int c = 0; c < p_Nq; ++c) {
            s_tmpU[es][c][j][i] = r_Ud[c];
            s_tmpV[es][c][j][i] = r_Vd[c];
            s_tmpW[es][c][j][i] = r_Wd[c];
            r_Ud[c] = 0;
            r_Vd[c] = 0;
            r_Wd[c] = 0;
          }
        }
      }
    } // es

    @barrier();

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
          if (b < p_Nq) {

#pragma unroll p_cubNq
            for (int j = 0; j < p_cubNq; ++j) {
              dfloat Ijb = s_cubInterp[j][b];
#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {
                r_Ud[c] += Ijb * s_tmpU[es][c][j][i];
                r_Vd[c] += Ijb * s_tmpV[es][c][j][i];
                r_Wd[c] += Ijb * s_tmpW[es][c][j][i];
              }
            }
          }
        }
      }
    } // es

    @barrier();

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
          if (b < p_Nq) {
            for (int c = 0; c < p_Nq; ++c) {
              s_tmpU[es][c][b][i] = r_Ud[c];
              s_tmpV[es][c][b][i] = r_Vd[c];
              s_tmpW[es][c][b][i] = r_Wd[c];
              r_Ud[c] = 0;
              r_Vd[c] = 0;
              r_Wd[c] = 0;
            }
          }
        }
      }
    }

    @barrier();

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
        for (int a = 0; a < p_cubNq; ++a; @inner(0)) {
          if (a < p_Nq && b < p_Nq && element != -1) {

#pragma unroll p_cubNq
            for (int i = 0; i < p_cubNq; ++i) {
              dfloat Iia = s_cubInterp[i][a];
#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {
                r_Ud[c] += Iia * s_tmpU[es][c][b][i];
                r_Vd[c] += Iia * s_tmpV[es][c][b][i];
                r_Wd[c] += Iia * s_tmpW[es][c][b][i];
              }
            }

            for (int c = 0; c < p_Nq; ++c) {

              const dlong id = element * p_Np + c * p_Nq * p_Nq + b * p_Nq + a;

#if p_MovingMesh
              // only modify output if mesh is moving
              dfloat bdivw = 0.0;
              dfloat invLMM = 0.0;
#pragma unroll p_nEXT
              for (int s = 0; s < p_nEXT; s++) {
                invLMM += s_c[s] * invLumpedMassMatrix[id + s * offset];
                bdivw += s_c[s] * BdivW[id + s * offset];
              }
              const dfloat Uxd = Ud[id + 0 * offset];
              const dfloat Uyd = Ud[id + 1 * offset];
              const dfloat Uzd = Ud[id + 2 * offset];

              NU[id + 0 * offset + NUoffset] = (r_Ud[c] - bdivw * Uxd) * invLMM;
              NU[id + 1 * offset + NUoffset] = (r_Vd[c] - bdivw * Uyd) * invLMM;
              NU[id + 2 * offset + NUoffset] = (r_Wd[c] - bdivw * Uzd) * invLMM;
#else
              dfloat invLMM = invLumpedMassMatrix[id];
              NU[id + 0 * offset + NUoffset] = (r_Ud[c]) * invLMM;
              NU[id + 1 * offset + NUoffset] = (r_Vd[c]) * invLMM;
              NU[id + 2 * offset + NUoffset] = (r_Wd[c]) * invLMM;
#endif
            }
          }
        }
      }
    } // es

  } // eo
}

