    //    @barrier("local");

    for(int t=0;t<p_BLOCKSIZE;++t;@inner(0)) {
      if(t<4) {
        s_ip[0*p_BLOCKSIZE+t] += s_ip[0*p_BLOCKSIZE+t+4];
