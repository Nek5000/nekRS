[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL]
polynomialOrder = 7
#startFrom = restart.fld
stopAt = endTime
endTime = 20

dt = 2.0e-2
timeStepper = bdf2
extrapolation = OIFS
targetCFL = 2.0

writeControl = runTime
writeInterval = 1

filtering = hpfrt
filterWeight = 1
filterCutoffRatio = 0.9 

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = null
density = 1.0
viscosity = -1600
residualTol = 1e-06
