          for(int m = 0; m < p_Nq; m++) {
            r_Auk   += s_D[m][j] * s_Gqs[m][i];
            r_Auk   += s_D[m][i] * s_Gqr[j][m];
          }

          const dlong id = element * p_Np + k * p_Nq * p_Nq + j * p_Nq + i;
