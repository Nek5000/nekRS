[GENERAL]
polynomialOrder = 7
#startFrom = "restart.fld"
stopAt = endTime
endTime = 20

dt = 2.0e-2
timeStepper = tombo2
extrapolation = subCycling
subCyclingSteps = 1

writeControl = runTime
writeInterval = 1

regularization = hpfrt + nModes = 1 + scalingCoeff = 0.02/${dt}

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
solver = cg+block
boundaryTypeMap = none
density = 1.0
viscosity = -1600
residualTol = 1e-06
