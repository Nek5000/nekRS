[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 9
stopAt = numSteps
numSteps = 1
dt = 1e-03
timeStepper = bdf2
writeInterval = 1

[PROBLEMTYPE]
equation = lowMachNS

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-06

[VELOCITY]
boundaryTypeMap = inlet, outlet
residualTol = 1e-08
density = 1
viscosity = 1

[TEMPERATURE] 
boundaryTypeMap = inlet, outlet
residualTol = 1e-12
rhoCp = 1
conductivity = 1 
