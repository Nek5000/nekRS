      subroutine useric (ix,iy,iz,ieg)
      include 'SIZE'
      include 'TOTAL'
      include 'NEKUSE'

      c1 = pi * 0.5/0.6

      arg = c1*z
      if (arg.gt.0.5*pi) then
        ux = 1.0
      else
        ux = sin(arg)
      endif

      uy=0.0
      uz=0.0

      return
      end
