[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 11 
stopAt = numSteps
numSteps = 300
dt = 1e-03
timeStepper = bdf2
writeInterval = 100

[PROBLEMTYPE]
equation = lowMachNS

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-10

[VELOCITY]
boundaryTypeMap = fixedValue
residualTol = 1e-12
density = 1
viscosity = 1

[TEMPERATURE] 
boundaryTypeMap = fixedValue
residualTol = 1e-12
rhoCp = 1
conductivity = 1 
