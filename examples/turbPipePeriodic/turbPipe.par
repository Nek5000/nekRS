[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL]
polynomialOrder = 7
#startFrom = restart.fld
stopAt = endTime
endTime = 200

dt = 6.0e-3
timeStepper = bdf2
extrapolation = OIFS
targetCFL = 3.8

writeControl = runTime
writeInterval = 20

filtering = hpfrt
filterWeight = 8
filterCutoffRatio = 0.9 

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = wall
viscosity = -19000
density = 1.0
residualTol = 1e-06
