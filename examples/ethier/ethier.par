[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7
#startFrom = restart.fld
stopAt = numSteps
numSteps = 3
dt = 2e-04
timeStepper = bdf1
writeControl = numSteps
writeInterval = 1

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-08

[VELOCITY]
residualTol = 1e-12
viscosity = -100 

[SCALAR01]
residualTol = 1e-12
diffusivity = -100  
