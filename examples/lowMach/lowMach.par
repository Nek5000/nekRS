[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7 
stopAt = numSteps
numSteps = 300
dt = 1e-03
timeStepper = tombo2
writeInterval = 100

[PRESSURE]
residualTol = 1e-08
residualProj = false

[VELOCITY]
boundaryTypeMap = fixedValue
residualTol = 1e-12

[TEMPERATURE] 
boundaryTypeMap = fixedValue
residualTol = 1e-12
