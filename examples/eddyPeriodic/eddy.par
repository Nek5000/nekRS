[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7
#startFrom = restart.fld
numSteps = 1000
dt = 1e-04
timeStepper = bdf2
writeInterval = 100

