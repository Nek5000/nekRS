                               @restrict const dfloat * a,
                               @restrict const dfloat * b,
                               @restrict const dfloat * c,
                               @restrict const dfloat * d,
                               @restrict       dfloat * ips){

  for(dlong n=0;n<Nblocks;++n;@outer(0)){

    @shared volatile dfloat s_ip[3*p_BLOCKSIZE];

