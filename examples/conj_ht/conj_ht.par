#
# nek parameter file
#
[GENERAL] 
polynomialOrder = 7
numSteps = 500 
dt = 2e-02
writeInterval = 100

[PRESSURE]
residualTol = 1e-06
residualProj = no

[VELOCITY]
boundaryTypeMap = v, O, W
residualTol = 1e-06
#density = 1
viscosity = 1/100.

[TEMPERATURE]
boundaryTypeMap = t, O, I
#rhoCp = 1.0
conductivity = 1/(10.*100) 
residualTol = 1e-06
residualProj = yes
