[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7
numSteps = 1500 
dt = 2e-02
writeInterval = 100

[PRESSURE]
residualTol = 1e-06
residualProj = no

[VELOCITY]
boundaryTypeMap = v, O, W
residualTol = 1e-06
#density = 1.0
viscosity = -1000

[TEMPERATURE]
boundaryTypeMap = t, O, I
rhoCp = 1.0
conductivity = -1000 
residualTol = 1e-06
