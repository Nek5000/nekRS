@kernel void setHelmholtzCoeff(const dlong N,
                               const dfloat lambda,
                               const dlong offset,
                               @restrict const  dfloat *PROP,
                               @restrict dfloat *coeff)
{
 for(dlong n=0;n<N;++n;@tile(256,@outer,@inner)){
    if(n<N){
      coeff[0*offset + n] = PROP[0*offset + n];
      coeff[1*offset + n] = lambda*PROP[1*offset + n];
    }
  }
}
