[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 9
stopAt = numSteps
numSteps = 2
dt = 1e-03
timeStepper = bdf2
writeInterval = 1

[PROBLEMTYPE]
equation = lowMachNS

[PRESSURE]
preconditioner = semg_amg
residualTol = 1e-08

[VELOCITY]
boundaryTypeMap = fixedValue
residualTol = 1e-10
density = 1
viscosity = 1

[TEMPERATURE] 
boundaryTypeMap = fixedValue
residualTol = 1e-10
rhoCp = 1
conductivity = 1 
