[OCCA]
backend = SERIAL
deviceNumber = LOCAL-RANK

[GENERAL]
polynomialOrder = 7
stopAt = numSteps
numSteps = 30
dt = 16e-4
timeStepper = tombo2
writeInterval = 50

extrapolation = subCycling
subCyclingSteps = 2

[MESH]
solver = user

[PRESSURE]
residualTol = 1e-08
#residualProj = false

[VELOCITY]
solver=pcg
residualTol = 1e-12
density = 1.0
viscosity = -100.0
