#if p_knl == 12

@kernel void subCycleStrongCubatureVolumeHex3D_v12(const dlong Nelements,
                                                   @restrict const dlong *elementList,
                                                   @restrict const dfloat *cubD,
                                                   @restrict const dfloat *cubInterpT,
                                                   const dlong offset,
                                                   const dlong cubatureOffset,
                                                   const dlong NUoffset,
                                                   @restrict const dfloat *invLumpedMassMatrix,
                                                   @restrict const dfloat *BdivW,
                                                   const dfloat c0,
                                                   const dfloat c1,
                                                   const dfloat c2,
                                                   @restrict const dfloat *conv,
                                                   @restrict const dfloat *Ud,
                                                   @restrict dfloat *NU)
{

#if p_cubNq == 16 || p_cubNq == 8
#define p_cubPad 1
#else
#define p_cubPad 0
#endif

#if p_Nq == 16 || p_Nq == 8 || p_Nq == 12
#define p_Pad 1
#else
#define p_Pad 0
#endif

#if p_cubNq == 3
#define p_Nblk 28
#elif p_cubNq == 4
#define p_Nblk 8
#elif p_cubNq == 5
  //#define p_Nblk 5
#define p_Nblk 10
#elif p_cubNq == 6
  //#define p_Nblk 3
#define p_Nblk 7
#elif p_cubNq == 7
  //#define p_Nblk 1
#define p_Nblk 5
#elif p_cubNq == 8
#define p_Nblk 2
#elif p_cubNq == 9
#define p_Nblk 3
#elif p_cubNq == 10
#define p_Nblk 1
#elif p_cubNq == 11
#define p_Nblk 1
#else
#define p_Nblk 1
#endif

  // (phi, U.grad Ud)
  for (dlong eo = 0; eo < Nelements; eo += p_Nblk; @outer(0)) {
    @shared dfloat s_cubD[p_cubNq][p_cubNq + p_cubPad];
    @shared dfloat s_cubInterp[p_cubNq][p_Nq + p_Pad];
    @exclusive dfloat r_Ud[p_Nq];

    @shared dfloat s_tmpU[p_Nblk][p_Nq][p_cubNq][p_cubNq + p_cubPad];

    @shared dfloat s_c[p_nEXT];
    @exclusive dlong element;
    @shared dfloat s_sliceU[p_Nblk][p_cubNq][p_cubNq + p_cubPad];

    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
          const int id = i + j * p_cubNq;
          dlong e = es + eo;
          element = (e < Nelements) ? elementList[e] : -1;

          if (es == 0) {
            if (i == 0 && j == 0) {
              //#pragma unroll p_nEXT
              for (int s = 0; s < p_nEXT; s++) {
                dfloat coeff = 0;
                if (s == 0)
                  coeff = c0;
                if (s == 1)
                  coeff = c1;
                if (s == 2)
                  coeff = c2;
                // r_c[s] = coeff;
                s_c[s] = coeff;
              }
            }

            if (id < p_Nq * p_cubNq) {
              //	  s_cubInterpT[j][i] = cubInterpT[id];
              s_cubInterp[i][j] = cubInterpT[id];
            }
            s_cubD[j][i] = cubD[id];
          }
        }
      }
    } // es

#define p_dim 3

#pragma unroll p_dim
    for (int d = 0; d < p_dim; ++d) {

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int a = 0; a < p_cubNq; ++a; @inner(0)) {
            if (a < p_Nq && b < p_Nq && element != -1) {

              for (int c = 0; c < p_Nq; ++c) {
                const dlong id = element * p_Np + c * p_Nq * p_Nq + b * p_Nq + a;
                s_tmpU[es][c][b][a] = Ud[id + d * offset];
              }
            }
          }
        }
      } // es

      @barrier();

      // interpolate in 'a'
      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

            if (b < p_Nq) {

#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {
                r_Ud[c] = 0;
              }

#pragma unroll p_Nq
              for (int a = 0; a < p_Nq; ++a) {
                const dfloat Iia = s_cubInterp[i][a];
#pragma unroll p_Nq
                for (int c = 0; c < p_Nq; ++c) {
                  r_Ud[c] += Iia * s_tmpU[es][c][b][a];
                }
              }
            }
          }
        }
      } // es

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

            if (b < p_Nq) {

              for (int c = 0; c < p_Nq; ++c) {
                s_tmpU[es][c][b][i] = r_Ud[c];
                r_Ud[c] = 0;
              }
            }
          }
        }
      } // es

      @barrier();

      // interpolate in 's'
      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

#pragma unroll p_Nq
            for (int c = 0; c < p_Nq; ++c) {
              r_Ud[c] = 0;
            }

            // interpolate in b
#pragma unroll p_Nq
            for (int b = 0; b < p_Nq; ++b) {
              const dfloat Ijb = s_cubInterp[j][b];
#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {
                r_Ud[c] += Ijb * s_tmpU[es][c][b][i];
              }
            }
          }
        }
      } // es

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            for (int c = 0; c < p_Nq; ++c) {
              s_tmpU[es][c][j][i] = r_Ud[c];
              r_Ud[c] = 0;
            }
          }
        }
      } // es

#ifndef gfxXX
#pragma unroll p_cubNq
#else
#pragma nounroll
#endif
      for (int k = 0; k < p_cubNq; ++k) {
        @barrier();

        for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
          for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
            for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

              dfloat Udk = 0;
#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {
                const dfloat Ikc = c_I[k][c];
                Udk += Ikc * s_tmpU[es][c][j][i];
              }
              s_sliceU[es][j][i] = Udk;
            }
          }
        } // es

        @barrier();

        for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
          for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
            for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
              if (element != -1) {

                dfloat Udr = 0, Uds = 0, Udt = 0;

#pragma unroll p_Nq
                for (int n = 0; n < p_Nq; ++n) {
                  dfloat Dkn = c_DI[k][n];

                  Udt += Dkn * s_tmpU[es][n][j][i];
                }

#pragma unroll p_cubNq
                for (int n = 0; n < p_cubNq; ++n) {
                  dfloat Din = s_cubD[i][n];
                  dfloat Djn = s_cubD[j][n];

                  Udr += Din * s_sliceU[es][j][n];
                  Uds += Djn * s_sliceU[es][n][i];
                }

                dfloat Uhat = 0.0;
                dfloat Vhat = 0.0;
                dfloat What = 0.0;

                const int id = element * p_cubNp + k * p_cubNq * p_cubNq + j * p_cubNq + i;

#pragma unroll p_nEXT
                for (int s = 0; s < p_nEXT; ++s) {
                  const int s_offset = s * p_NVfields * cubatureOffset;
                  Uhat += s_c[s] * conv[id + 0 * cubatureOffset + s_offset];
                  Vhat += s_c[s] * conv[id + 1 * cubatureOffset + s_offset];
                  What += s_c[s] * conv[id + 2 * cubatureOffset + s_offset];
                }

                // I_f^t*(J_f*C_f^t)*G_f*\hat{D}_f*I_f*u
                dfloat foo1 = Uhat * Udr + Vhat * Uds + What * Udt;
#pragma unroll p_Nq
                for (int c = 0; c < p_Nq; ++c) {
                  dfloat Ikc = c_I[k][c];
                  r_Ud[c] += Ikc * foo1;
                }
              }
            }
          }
        } // es
      }   // k

      // now project back in 'b'
      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

#pragma unroll p_Nq
            for (int c = 0; c < p_Nq; ++c) {
              s_tmpU[es][c][j][i] = r_Ud[c];
              r_Ud[c] = 0;
            }
          }
        }
      } // es

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            if (b < p_Nq) {

#pragma unroll p_cubNq
              for (int j = 0; j < p_cubNq; ++j) {
                dfloat Ijb = s_cubInterp[j][b];
#pragma unroll p_Nq
                for (int c = 0; c < p_Nq; ++c) {
                  r_Ud[c] += Ijb * s_tmpU[es][c][j][i];
                }
              }
            }
          }
        }
      } // es

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            if (b < p_Nq) {
              for (int c = 0; c < p_Nq; ++c) {
                s_tmpU[es][c][b][i] = r_Ud[c];
                r_Ud[c] = 0;
              }
            }
          }
        }
      }

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int a = 0; a < p_cubNq; ++a; @inner(0)) {
            if (a < p_Nq && b < p_Nq && element != -1) {

#pragma unroll p_cubNq
              for (int i = 0; i < p_cubNq; ++i) {
                dfloat Iia = s_cubInterp[i][a];
#pragma unroll p_Nq
                for (int c = 0; c < p_Nq; ++c) {
                  r_Ud[c] += Iia * s_tmpU[es][c][b][i];
                }
              }

#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {

                const dlong id = element * p_Np + c * p_Nq * p_Nq + b * p_Nq + a;

#if p_MovingMesh
                // only modify output if mesh is moving
                dfloat bdivw = 0.0;
                dfloat invLMM = 0.0;
#pragma unroll p_nEXT
                for (int s = 0; s < p_nEXT; s++) {
                  invLMM += s_c[s] * invLumpedMassMatrix[id + s * offset];
                  bdivw += s_c[s] * BdivW[id + s * offset];
                }
                const dfloat Uxd = Ud[id + d * offset];
                NU[id + d * offset + NUoffset] = (r_Ud[c] - bdivw * Uxd) * invLMM;
#else
                dfloat invLMM = invLumpedMassMatrix[id];
                NU[id + d * offset + NUoffset] = (r_Ud[c]) * invLMM;
#endif
              }
            }
          }
        }
      } // es
    }   // d
  }     // eo
}

#endif
#if p_knl == 13

@kernel void subCycleStrongCubatureVolumeHex3D_v13(const dlong Nelements,
                                                   @restrict const dlong *elementList,
                                                   @restrict const dfloat *cubD,
                                                   @restrict const dfloat *cubInterpT,
                                                   const dlong offset,
                                                   const dlong cubatureOffset,
                                                   const dlong NUoffset,
                                                   @restrict const dfloat *invLumpedMassMatrix,
                                                   @restrict const dfloat *BdivW,
                                                   const dfloat c0,
                                                   const dfloat c1,
                                                   const dfloat c2,
                                                   @restrict const dfloat *conv,
                                                   @restrict const dfloat *Ud,
                                                   @restrict dfloat *NU)
{

#if p_cubNq == 16 || p_cubNq == 8
#define p_cubPad 1
#else
#define p_cubPad 0
#endif

#if p_Nq == 16 || p_Nq == 8 || p_Nq == 12
#define p_Pad 1
#else
#define p_Pad 0
#endif

#if p_cubNq == 3
#define p_Nblk 28
#elif p_cubNq == 4
#define p_Nblk 8
#elif p_cubNq == 5
  //#define p_Nblk 5
#define p_Nblk 10
#elif p_cubNq == 6
  //#define p_Nblk 3
#define p_Nblk 7
#elif p_cubNq == 7
  //#define p_Nblk 1
#define p_Nblk 5
#elif p_cubNq == 8
#define p_Nblk 2
#elif p_cubNq == 9
#define p_Nblk 3
#elif p_cubNq == 10
#define p_Nblk 1
#elif p_cubNq == 11
#define p_Nblk 1
#else
#define p_Nblk 1
#endif

  // (phi, U.grad Ud)
  for (dlong eo = 0; eo < Nelements; eo += p_Nblk; @outer(0)) {
    @shared dfloat s_cubD[p_cubNq][p_cubNq + p_cubPad];
    @shared dfloat s_cubInterp[p_cubNq][p_Nq + p_Pad];
    @exclusive dfloat r_Ud[p_cubNq];

    @shared dfloat s_tmpU[p_Nblk][p_cubNq][p_cubNq][p_cubNq + p_cubPad];

    @shared dfloat s_c[p_nEXT];
    @exclusive dlong element;
 
    for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
      for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
        for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
          const int id = i + j * p_cubNq;
          dlong e = es + eo;
          element = (e < Nelements) ? elementList[e] : -1;
          // element = e;

          if (es == 0) {
            if (i == 0 && j == 0) {
              //#pragma unroll p_nEXT
              for (int s = 0; s < p_nEXT; s++) {
                dfloat coeff = 0;
                if (s == 0)
                  coeff = c0;
                if (s == 1)
                  coeff = c1;
                if (s == 2)
                  coeff = c2;
                // r_c[s] = coeff;
                s_c[s] = coeff;
              }
            }

            if (id < p_Nq * p_cubNq) {
              s_cubInterp[i][j] = cubInterpT[id];
            }
            s_cubD[j][i] = cubD[id];
          }
        }
      }
    } // es

#define p_dim 3

    //#pragma unroll p_dim
#pragma nounroll
    for (int d = 0; d < p_dim; ++d) {
      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int a = 0; a < p_cubNq; ++a; @inner(0)) {
            if (a < p_Nq && b < p_Nq && element != -1) {

              for (int c = 0; c < p_Nq; ++c) {
                const dlong id = element * p_Np + c * p_Nq * p_Nq + b * p_Nq + a;
                s_tmpU[es][c][b][a] = Ud[id + d * offset];
              }
            }
          }
        }
      } // es

      @barrier();

      // interpolate in 'a'
      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int c = 0; c < p_cubNq; ++c; @inner(1)) {
          for (int b = 0; b < p_cubNq; ++b; @inner(0)) {

            if (b < p_Nq && c < p_Nq) {

#pragma unroll p_cubNq
              for (int i = 0; i < p_cubNq; ++i) {
                r_Ud[i] = 0;
              }
#pragma unroll p_Nq
              for (int a = 0; a < p_Nq; ++a) {
                dfloat ucba = s_tmpU[es][c][b][a];
#pragma unroll p_cubNq
                for (int i = 0; i < p_cubNq; ++i) {
                  const dfloat Iia = c_I[i][a]; // nice broadcast
                  r_Ud[i] += Iia * ucba;
                }
              }

              for (int i = 0; i < p_cubNq; ++i) {
                s_tmpU[es][c][b][i] = r_Ud[i];
              }
            }
          }
        }
      } // es

      @barrier();

      // interpolate in 'b'
      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int c = 0; c < p_cubNq; ++c; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            if (c < p_Nq) {

#pragma unroll p_cubNq
              for (int j = 0; j < p_cubNq; ++j) {
                r_Ud[j] = 0;
              }
#pragma unroll p_Nq
              for (int b = 0; b < p_Nq; ++b) {
                dfloat ucbi = s_tmpU[es][c][b][i];
#pragma unroll p_cubNq
                for (int j = 0; j < p_cubNq; ++j) {
                  const dfloat Ijb = c_I[j][b]; // nice broadcast
                  r_Ud[j] += Ijb * ucbi;
                }
              }

              for (int j = 0; j < p_cubNq; ++j) {
                s_tmpU[es][c][j][i] = r_Ud[j];
              }
            }
          }
        }
      } // es

      @barrier();
      
      // interp in 'c'
      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {

#pragma unroll p_cubNq
            for (int k = 0; k < p_cubNq; ++k) {
              r_Ud[k] = 0;
            }
#pragma unroll p_Nq
            for (int c = 0; c < p_Nq; ++c) {
              dfloat ucji = s_tmpU[es][c][j][i];
#pragma unroll p_cubNq
              for (int k = 0; k < p_cubNq; ++k) {
                const dfloat Ikc = c_I[k][c]; // nice broadcast
                r_Ud[k] += Ikc * ucji;
              }
            }

            for (int k = 0; k < p_cubNq; ++k) {
              s_tmpU[es][k][j][i] = r_Ud[k];
            }

            for (int c = 0; c < p_Nq; ++c) {
              r_Ud[c] = 0;
            }
          }
        }
      } // es

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            if (element != -1) {

#pragma nounroll
              for (int k = 0; k < p_cubNq; ++k) {
                dfloat Udr = 0, Uds = 0, Udt = 0;

#pragma unroll p_cubNq
                for (int n = 0; n < p_cubNq; ++n) {
                  dfloat Dkn = c_D[k][n];
                  dfloat Din = s_cubD[i][n];
                  dfloat Djn = s_cubD[j][n];

                  Udt += Dkn * s_tmpU[es][n][j][i];
                  Udr += Din * s_tmpU[es][k][j][n];
                  Uds += Djn * s_tmpU[es][k][n][i];
                }

                dfloat Uhat = 0.0;
                dfloat Vhat = 0.0;
                dfloat What = 0.0;

                const int id = element * p_cubNp + k * p_cubNq * p_cubNq + j * p_cubNq + i;

#pragma unroll p_nEXT
                for (int s = 0; s < p_nEXT; ++s) {
                  const int s_offset = s * p_NVfields * cubatureOffset;
                  Uhat += s_c[s] * conv[id + 0 * cubatureOffset + s_offset];
                  Vhat += s_c[s] * conv[id + 1 * cubatureOffset + s_offset];
                  What += s_c[s] * conv[id + 2 * cubatureOffset + s_offset];
                }

                // I_f^t*(J_f*C_f^t)*G_f*\hat{D}_f*I_f*u
                dfloat foo1 = Uhat * Udr + Vhat * Uds + What * Udt;
#pragma unroll p_Nq
                for (int c = 0; c < p_Nq; ++c) {
                  dfloat Ikc = c_I[k][c];
                  r_Ud[c] += Ikc * foo1;
                }
              }
            }
          } // i
        }   // j
      }     // es

      @barrier();

      // now project back in 'b'
      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int j = 0; j < p_cubNq; ++j; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            for (int c = 0; c < p_Nq; ++c) {
              s_tmpU[es][c][j][i] = r_Ud[c];
            }
          }
        }
      }

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int c = 0; c < p_cubNq; ++c; @inner(1)) {
          for (int i = 0; i < p_cubNq; ++i; @inner(0)) {
            if (c < p_Nq) {
#pragma unroll p_cubNq
              for (int b = 0; b < p_Nq; ++b) {
                r_Ud[b] = 0;
              }

#pragma unroll p_cubNq
              for (int j = 0; j < p_cubNq; ++j) {
                dfloat ucji = s_tmpU[es][c][j][i];
#pragma unroll p_Nq
                for (int b = 0; b < p_Nq; ++b) {
                  const dfloat Ijb = c_I[j][b]; // nice broadcast
                  r_Ud[b] += Ijb * ucji;
                }
              }

              for (int b = 0; b < p_Nq; ++b) {
                s_tmpU[es][c][b][i] = r_Ud[b];
                r_Ud[b] = 0;
              }
            }
          }
        }
      } // es

      @barrier();

      // project back in 'a'

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int c = 0; c < p_cubNq; ++c; @inner(1)) {
          for (int b = 0; b < p_cubNq; ++b; @inner(0)) {

            if (c < p_Nq && b < p_Nq) {
#pragma unroll p_cubNq
              for (int a = 0; a < p_Nq; ++a) {
                r_Ud[a] = 0;
              }

#pragma unroll p_cubNq
              for (int i = 0; i < p_cubNq; ++i) {
                dfloat ucbi = s_tmpU[es][c][b][i];
#pragma unroll p_Nq
                for (int a = 0; a < p_Nq; ++a) {
                  const dfloat Iia = c_I[i][a]; // nice broadcast
                  r_Ud[a] += Iia * ucbi;
                }
              }

              for (int a = 0; a < p_Nq; ++a) {
                s_tmpU[es][c][b][a] = r_Ud[a];
              }
            }
          }
        }
      }

      @barrier();

      for (int es = 0; es < p_Nblk; ++es; @inner(2)) {
        for (int b = 0; b < p_cubNq; ++b; @inner(1)) {
          for (int a = 0; a < p_cubNq; ++a; @inner(0)) {
            if (a < p_Nq && b < p_Nq && element != -1) {
#pragma unroll p_Nq
              for (int c = 0; c < p_Nq; ++c) {

                const dlong id = element * p_Np + c * p_Nq * p_Nq + b * p_Nq + a;

#if p_MovingMesh
                // only modify output if mesh is moving
                dfloat bdivw = 0.0;
                dfloat invLMM = 0.0;
#pragma unroll p_nEXT
                for (int s = 0; s < p_nEXT; s++) {
                  invLMM += s_c[s] * invLumpedMassMatrix[id + s * offset];
                  bdivw += s_c[s] * BdivW[id + s * offset];
                }
                const dfloat Uxd = Ud[id + d * offset];
                NU[id + d * offset + NUoffset] = (s_tmpU[es][c][b][a] - bdivw * Uxd) * invLMM;
#else
                dfloat invLMM = invLumpedMassMatrix[id];
                NU[id + d * offset + NUoffset] = (s_tmpU[es][c][b][a]) * invLMM;
#endif
              }
            }
          }
        }
      } // es
      
    }   // d
  }     // eo
}

