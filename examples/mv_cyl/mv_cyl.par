[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL]
#verbose = true
polynomialOrder = 7
stopAt = numSteps
#numSteps = 1000
numSteps = 10
dt = 1e-3
timeStepper = tombo3
writeInterval = 50

[PROBLEMTYPE]
stressFormulation = true

[PRESSURE]
residualTol = 1e-10
residualProj = false

[VELOCITY]
residualTol = 1e-12
residualProj = false

[TEMPERATURE]
residualTol = 1e-12
residualProj = false
