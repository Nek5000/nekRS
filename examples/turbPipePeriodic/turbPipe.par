[GENERAL]
#verbose = true
polynomialOrder = 7
#startFrom = "restart.fld"
stopAt = endTime
endTime = 200

dt = 6.0e-3
timeStepper = tombo2
subCyclingSteps = 2

writeControl = simulationTime 
writeInterval = 20

regularization = hpfrt + nModes=1 + scalingCoeff=10
constFlowRate = meanVelocity=1.0 + direction=Z

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = wall
viscosity = -19000
density = 1.0
residualTol = 1e-06

[CASEDATA]
Re_tau = 550
zLength = 6
