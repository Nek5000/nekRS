[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL]
polynomialOrder = 7
#startFrom = restart.fld
stopAt = endTime
endTime = 200

dt = 6.0e-3
timeStepper = bdf2
extrapolation = OIFS
targetCFL = 3.8

writeControl = runTime
writeInterval = 1

filtering = hpfrt
filterWeight = 8
filterCutoffRatio = 0.9 

[PRESSURE]
preconditioner = semg_amg
#amgSolver = parAlmond
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = inlet, outlet, wall
density = 1.0
viscosity = -19000
residualTol = 1e-06
