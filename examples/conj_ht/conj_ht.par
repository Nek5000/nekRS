[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL] 
#startFrom = restart.fld
polynomialOrder = 7
numSteps = 1000 
dt = 2e-02
writeInterval = 100

[PROBLEMTYPE]
variableProperties = yes

[PRESSURE]
residualTol = 1e-06
residualProj = no

[VELOCITY]
boundaryTypeMap = v, O, W
residualTol = 1e-08

[TEMPERATURE]
boundaryTypeMap = t, O, I
residualTol = 1e-08
